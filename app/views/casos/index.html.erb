<h1>Listing casos</h1>

<table>
  <thead>
    <tr>
      <th>Casename</th>
      <th>Victname</th>
      <th>Date occu</th>
      <th>Place occu</th>
      <th>Desc</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @casos.each do |caso| %>
      <tr>
        <td><%= caso.casename %></td>
        
        <td><%= caso.victname %></td>
        <td><%= caso.date_occu %></td>
        <td><%= caso.place_occu %></td>
        <td><%= caso.desc %></td>
        <td><%= caso.status %></td>
        <td><%= caso.user_id %></td>
        <td><%= link_to 'Show', caso %></td>
        <td><%= link_to 'Edit', edit_caso_path(caso) %></td>
        <td><%= link_to 'Destroy', caso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caso', new_caso_path %>
